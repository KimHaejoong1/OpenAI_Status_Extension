{
  "manifest_version": 3,
  "name": "OpenAI Status Reader",
  "version": "1.0.1",
  "description": "A simple OpenAI Status Reader Chrome extension.",
  "icons": {
    "16": "images/icon16.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  },
  "action": {
    "default_popup": "popup.html"
  },
  "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'self'; connect-src https://status.openai.com"
  }
}
